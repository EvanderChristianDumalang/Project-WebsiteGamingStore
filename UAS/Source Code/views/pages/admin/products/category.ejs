<!DOCTYPE html>
<html lang="en">

<%- include('../../../partials/admin/head'); %>


  <body onload="startTime()">

    <%- include('../../../partials/admin/sidenav'); %>
    <%- include('../../../partials/admin/header',{name:name}); %>
  
    <div class="content">
      <h1 style="font-family: 'Poppins';" class="text-center">Tambah Category</h1>
      <div class="container">

        <form action="/dashboard/product/category/create" method="POST">
          <div class="form-group">
            <label for="title">Judul Kategori : </label>
            <input type="text" class="form-control" id="title" name="title" placeholder="" value="<%= category.title %>">
          </div>
          <div class="form-group">
        <button class="btn btn-primary" type="submit">Submit</button>
        <a href="/dashboard/product/create" class="btn btn-secondary">Cancel</a>
          </div>
        </form>
  
      </div>
      </div>  

      <%- include('../../../partials/admin/footer'); %>
  </body>


</html>